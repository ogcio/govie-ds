{
  "namedInputs": {
    "default": ["{projectRoot}/**/*"],
    "nonTest": ["!{projectRoot}/**/*.test.tsx"]
  },
  "targetDefaults": {
    "build": {
      "inputs": ["default", "^nonTest"],
      "dependsOn": ["^build"]
    },
    "dev": {
      "dependsOn": ["^build"]
    },
    "test": {
      "inputs": ["default", "^nonTest"]
    }
  },
  "implicitDependencies": {
    ".gitignore": "*",
    "nx.json": "*",
    "package.json": "*",
    "pnpm-workspace.yaml": "*",
    ".github/**/*": "*"
  }
}
