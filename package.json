{
  "name": "root",
  "private": true,
  "packageManager": "pnpm@9.12.3",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "format": "nx run-many --target=format --all",
    "format:check": "nx run-many --target=format:check --all",
    "lint": "nx run-many --target=lint --all",
    "graph": "nx graph",
    "ds": "nx run @govie-ds/docs:dev",
    "ds:build": "nx run @govie-ds/docs:build",
    "ds:build:prod": "nx run @govie-ds/docs:build:prod",
    "ds:deploy": "nx run @govie-ds/docs:docker --parallel=1 --configuration=$CONFIGURATION",
    "ds:storybook": "nx run @govie-ds/docs:storybook",
    "build": "nx run-many --target=build --all",
    "build:libs": "nx run-many --target=build --all --exclude=@govie-ds/docs,@govie-ds/examples-vite,@govie-ds/examples-nextjs",
    "test": "nx run-many --target=test --all --exclude=@govie-frontend/storybook,@govie-react/storybook --verbose",
    "tokens:build": "nx run @govie-ds/tokens:build",
    "govie:build": "nx run @govie-ds/theme-govie:build",
    "figma:build": "nx run @govie-ds/figma:build",
    "html:storybook": "nx run @govie-frontend/storybook:dev",
    "html:storybook:build": "nx run @govie-frontend/storybook:build",
    "html:storybook:test": "nx run @govie-frontend/storybook:test",
    "react:storybook": "nx run @govie-react/storybook:dev",
    "react:storybook:build": "nx run @govie-react/storybook:build",
    "react:storybook:test": "nx run @govie-react/storybook:test",
    "examples:vite": "nx run @govie-ds/examples-vite:dev",
    "examples:nextjs": "nx run @govie-ds/examples-nextjs:dev"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "3.2.2",
    "@storybook/addon-a11y": "^8.5.0-alpha.21",
    "@storybook/addon-coverage": "1.0.4",
    "@storybook/addon-docs": "^8.5.0-alpha.21",
    "@storybook/addon-essentials": "^8.5.0-alpha.21",
    "@storybook/addon-links": "^8.5.0-alpha.21",
    "@storybook/addon-onboarding": "^8.5.0-alpha.21",
    "@storybook/addon-viewport": "^8.5.0-alpha.21",
    "@storybook/blocks": "^8.5.0-alpha.21",
    "@storybook/experimental-addon-test": "^8.5.0-alpha.21",
    "@storybook/nextjs": "^8.5.0-alpha.21",
    "@storybook/react": "^8.5.0-alpha.21",
    "@storybook/react-vite": "^8.5.0-alpha.21",
    "@storybook/test": "^8.5.0-alpha.21",
    "@storybook/test-runner": "0.19.1",
    "@tailwindcss/typography": "^0.5.15",
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "22.8.4",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitest/browser": "^2.1.8",
    "axe-playwright": "2.0.3",
    "chromatic": "11.16.3",
    "concurrently": "^9.0.1",
    "http-server": "^14.1.1",
    "nx": "^19.5.6",
    "playwright": "^1.48.1",
    "prettier": "^3.3.3",
    "storybook": "^8.5.0-alpha.21",
    "storybook-addon-pseudo-states": "^4.0.2",
    "tailwind-variants": "^0.2.1",
    "tailwindcss": "3.4.14",
    "tsup": "^8.2.5",
    "tsx": "^4.19.2",
    "typescript": "5.6.3",
    "vite": "5.4.10",
    "vitest": "2.1.8",
    "wait-on": "^8.0.1"
  },
  "peerDependencies": {
    "@vitest/spy": "2.1.3"
  },
  "dependencies": {}
}
